{
	"name":"futGameHubFlow"
	,"initial":
	{
		"targets":["hubEntry"]
	}
	,"states":
	[
		{
			"name":"hubEntry" /* Step 1: Live message screen comes first */
			,"type":"external"
			,"file":"/fut/futLiveMessaging.nav"
			,"outputs":
			{
				"advance":"gameHubCheckpoint"
			}
		}
		,{
			"name":"gameHubCheckpoint"
			,"onEnter":
			[
				["loadViewModel",["futgamehubcheckpointviewmodel"]]
			]
			,"onExit":
			[
				["unloadViewModel",["futgamehubcheckpointviewmodel"]]
			]
			,"transitions":
			[
				{
					"event":"goToOnboardingVideo"
					,"targets":["onboardingVideo"]
				}				
				,{
					"event":"advance"
					,"targets":["CheckTakeMeThereLink"]
				}				
			]
		}
		,{
			"name":"onboardingVideo"
			,"onEnter":
			[
				["loadView",["screen", "external/ion_fut/screens/futStarterSquadIntro"]]
			]
			,"onExit":
			[
				["unloadView",["screen", "external/ion_fut/screens/futStarterSquadIntro"]]
			]
			,"transitions":
			[
				{
					"event":"advance"
					,"targets":["CheckTakeMeThereLink"]
				}				
			]
		}
		,{
			"name":"CommonGamehubTransitions"
			,"states":
			[
				{
					"name":"CheckTakeMeThereLink" /* Step 2: Check for any deep links that needs to be processed */
					,"onEnter":
					[
						["sendScreenEvent", ["ResetFELoadingIcon"]]
						,["sendScreenEvent", ["CheckTakeMeThereLink"]]
					]
					,"transitions":
					[
						{
							"event":"advance"
							,"targets":["hub"]
						}
					]
				}
				,{
					"name":"hubCentral_Wrapper" /* Step 3: the actual hub! */
					,"onEnter":
					[
						["loadViewModel",["FluxViewModel"]]
						,["loadViewModel",["futgamehubviewmodel"]]
						,["loadViewModel",["futleaderboardsviewmodel", "1"]]
						,["loadViewModel",["MainMenuMoviePlayerViewModel"]]
						,["sendScreenEvent", ["ResetFELoadingIcon"]]
						,["sendAction",["setStadiumPosition", ".StadiumFECameras.STADIUM_FE_CAMERA_STEP_3"]]
						,["sendAction",["evt_hub_show_event"]]
						,["sendAction",["evt_hub_update_event", ".NavFIFAHubBackgroundType.FUT_MODE_BACKGROUND"]]
						,["loadView",["background", "ANIMATION", "3"]]
					]
					,"onExit":
					[
						["unloadViewModel",["futleaderboardsviewmodel"]]
						,["unloadViewModel",["futgamehubviewmodel"]]
						,["unloadViewModel",["MainMenuMoviePlayerViewModel"]]
						,["unloadViewModel",["FluxViewModel"]]
						,["unloadView",["background"]]
					]
					,"states":
					[
						{
							"name":"gamehub_wrapper"
							,"onEnter":
							[
								["loadView",["screen", "external/ion_fut/screens/gameHub/GameHub"]]
							]
							,"onExit":
							[
								["unloadView",["screen", "external/ion_fut/screens/gameHub/GameHub"]]
							]
							,"states":
							[
								{
									"name":"hub"
									,"transitions":
									[
										{
											"event":"evt_set_fullscreen",
											"targets":["EATVFullScreen"]
										}
										,{
											"event":"evt_play_eatv",
											"targets":["EATVPlaying"]
										}
										,{
											"event":"evt_popup_enter",
											"targets":["idleOnMenuNoScreenSaver"]
										}
										,{
											"event":"ShowSquadReportPopup", 
											"targets":["showSquadReportPopup"]
										}
										,{
											"event":"ShowClubReportPopup", 
											"targets":["showClubReportPopup"]
										}
										,{
											"event":"GotoClaimedRewardScreen", 
											"targets":["showRewards"]
										}
										,{
											"event":"ShowRefreshRecommendedObjectivePopup",
											"targets":["showRefreshRecommendedObjectivePopup"]
										}
									]
								}
								,{
									"name":"hub_none"
								}
								,{
									"name":"EATVFullScreen"
									,"transitions":
									[
										{
											"event":"back"
											,"targets":["hub"]
										}
									]
								}
								,{
									"name":"EATVPlaying"
									,"transitions":
									[
										{
											"event":"evt_stop_eatv"
											,"targets":["hub"]
										}
										,{
											"event":"evt_set_fullscreen"
											, "targets":["EATVFullScreen"]
										}
									]
								}
								,{
									"name":"idleOnMenuNoScreenSaver"
									,"transitions":
									[
										{
											"event":"evt_popup_exit"
											,"targets":["hub"]
										}
									]
								}
								,{
									"name":"showSquadReportPopup"
									,"onEnter":
									[
										["loadView", ["popup", "AbuseReportingMessageSquad", "ABUSE_REPORTING_SQUAD_REPORT|OK|back"]]
									]
									,"onExit":
									[
										["unloadView", ["popup", "AbuseReportingMessageSquad"]]
									]
									,"transitions":
									[
										{
											"event":"back"
											,"targets":["hub"]
											,"actions":[ ["sendAction", ["checkGameHubStates"]] ]
										}
									]
								}
								,{
									"name":"showClubReportPopup"
									,"onEnter":
									[
										["loadView", ["popup", "AbuseReportingMessageClub", "ABUSE_REPORTING_CLUB_REPORT|OK|back"]]
										,["invoke", ["evt_invite_flow_not_ready"]]
									]
									,"onExit":
									[
										["unloadView", ["popup", "AbuseReportingMessageClub"]]
									]
									,"transitions":
									[
										{
											"event":"back"
											,"targets":["ChangeClubNameReported"]
										}
									]
								}
								,{
									"name":"showRefreshRecommendedObjectivePopup"
									,"onEnter":
									[
										["loadView", ["popup", "RefreshRecommendedObjectivePopup", "FUT_SCMP_REFRESH_RECOMMENDED|OK|GotoLoadingPopup"]]
									]
									,"onExit":
									[
										["unloadView", ["popup", "RefreshRecommendedObjectivePopup"]]
									]
									,"transitions":
									[
										{
											"event":"GotoLoadingPopup"
											,"targets":["showLoadingPopup"]
											,"actions":[ ["sendAction", ["futRefreshRecommendedObjective"]] ]
										}
									]
								}
								,{
									"name":"showLoadingPopup"
									,"onEnter":
									[
										["loadView", ["popup", "FUT_SCMP_LOADING_POPUP_NAME", "FUT_SCMP_LOADING"]]
									]
									,"onExit":
									[
										["unloadView", ["popup", "FUT_SCMP_LOADING_POPUP_NAME"]]
									]
									,"transitions":
									[
										{
											"event":"DismissLoadingPopup"
											,"targets":["hub"]
										}
									]
								}
							]
						}
						,{
							"name":"showRewards"
							,"onEnter":
							[
								["loadView", ["screen", "external/ion_fut/screens/futRewardsScreen"]]
							]
							,"onExit":
							[
								["unloadView", ["screen", "external/ion_fut/screens/futRewardsScreen"]]
							]
							,"transitions":
							[
								{
									"event":"advance"
									,"targets":["hub"]
								}
							]
						}
					]
				}
			]
			,"transitions":
			[
				/* Any new links here should also be added in FUTGameHubDataHelper::TakeMeThereTransition() */
				{

				,{
					"event":"GotoSimMatch"
					,"targets":["simMatch"]
				}
				,{
					"event":"UpsellStore"
					,"targets":["upsellFlow"]
				}
				/* Any new links here should also be added in FUTGameHubDataHelper::TakeMeThereTransition() */
				,{
					"event":"GotoSquadBattle"
					,"targets":["squadBattle"]
				}
				,{
					"event":"GotoSavedInput"
					,"targets":["savedInput"]
				}
				,{
					"event":"GotoFirstPartyUpsell"
					,"targets":["launchFirstPartyUpsell"]

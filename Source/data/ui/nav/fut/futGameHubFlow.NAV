		,{
			"name":"leaderboards"
			,"onEnter":
			[
				["loadView",["screen", "external/ion_fut/screens/FutLeaderboards"]]
				,["loadViewModel",["futleaderboardsviewmodel", "0"]]
			]
			,"onExit":
			[

				["unloadViewModel",["futleaderboardsviewmodel"]]
				,["unloadView",["screen", "external/ion_fut/screens/FutLeaderboards"]]
			]
			,"states":
			[		
				{
					"name":"leaderboardsInitialState"
					,"transitions":
					[
						{
							"event":"OpenUserActionsOverlay",
							"targets":["UserActionsOverlay"] 
						}
						,{
							"event":"back"
							,"targets":["hubEntry"]
						}
					]
				} 
				,{
					"let":{"AbuseReportingViewModel":"futabusereportingviewmodel", "AbuseReportingViewModelParam":"MyClubLeaderboards"}
					,"name":"UserActionsOverlay"
					,"states":
					[
						{
							"type":"external"
							,"file":"/online/abuseReportFlow.nav"
							,"outputs":
							{
								"out_back":"leaderboardsInitialState"
							}
						}
					]
				}
			]
		}
		,{
			"name":"EATrax"
			,"onEnter":
			[
				["loadViewModel",["FluxViewModel"]]
                ,["loadViewModel",["EaTraxViewModel", ".TraxView"]]
				,["loadView",["screen", "game/screens/fluxHub/FluxHub"]]
			]
			,"onExit":
			[
				["unloadView",["screen", "game/screens/fluxHub/FluxHub"]]
				,["unloadViewModel",["EaTraxViewModel"]]
				,["unloadViewModel",["FluxViewModel"]]
			]
			,"transitions":
			[
				
			]
			,"states":
			[
				{
					"name":"eaTraxList",
					"transitions":
					[
						{
							"event":"ConfirmToggleRequested"
							,"targets":["confirmTogglePopup"]
						}
						,{
							"event":"back"
							,"targets":["hubEntry"]
						}
					]
				}
                ,{
					"name":"confirmTogglePopup"
					,"onEnter":
					[
						["loadView", ["popup", "ConfirmTogglePopup", "TXT_EA_TRAX_ON_OFF_CONFIRMATION|TXT_YES|popupYes|TXT_NO|popupNo"]]
					]
					,"onExit":
					[
						["unloadView", ["popup", "ConfirmTogglePopup"]]
					]
					,"transitions":
					[
						{
							"event":"popupYes"
							,"actions":[ ["sendAction", ["updateScreen", "yes"] ] ]
							,"targets":["eaTraxList"]
						}
						,{
							"event":"popupNo"
							,"targets":["eaTraxList"]
						}
					]
				}
			]
		}

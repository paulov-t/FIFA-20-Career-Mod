				}
				,{
					"event":"changeClubName"
					,"targets":["ChangeClubName"]
				}
				,{
					"event":"GotoSquad"
					,"targets":["futSquads"]
				}
				/* Any new links here should also be added in FUTGameHubDataHelper::TakeMeThereTransition() */
				,{
					"event":"GotoSquadSelect"
					,"targets":["squadSelect"]
				}
				,{
					"event":"GotoOnlineGame"
					,"targets":["onlineGame"]
				}
				,{
					"event":"GotoDraftOnline"
					,"targets":["draftModeOnline"]
				}
				,{
					"event":"GotoDraftOffline"

{
	"name":"pauseParentFlow"
	,"onEnter":
	[
		["loadViewModel", ["InGameOverlayViewModel"]]
	]
	,"onExit":
	[
		["unloadViewModel", ["InGameOverlayViewModel"]]
	]
	,"states":
	[
		{
			"name":"pauseMenuFlow"
			,"initial":
			{
				"targets":["PMASkillGameCheck"]
			}

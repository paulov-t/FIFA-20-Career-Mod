									"event":"CareerMatchResults"
									,"targets":["CareerMatchResults"]
								}
								,{
									"event":"CareerMatchResultsPostGame"
									,"targets":["CareerMatchResultsPostGame"]
								}
								,{
									"event":"ManOfTheMatch"
									,"targets":["PlayerPerformanceFromGame"]
								}
								,{
									"event":"ManOfTheMatchPlayerCareer"
									,"targets":["ManOfTheMatchPlayerCareer"]
								}
								,{
									"event":"ManOfTheMatchClubs"
									,"targets":["ManOfTheMatchClubs"]
								}
								,{
									"event":"ManOfTheMatchClubsCup"
									,"targets":["ManOfTheMatchClubsCup"]
								}
								,{
									"event":"OnlineSeasonsSummary"

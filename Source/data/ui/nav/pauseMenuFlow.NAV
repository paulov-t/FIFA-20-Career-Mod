									"name":"EndGamePauseMenuConditionalCheck"
									,"onEnter":
									[
										["sendAction", ["condition", "shouldShowEndGamePauseMenu"]]
									]
									,"transitions":
									[
										{ "event":"true","actions": [["invoke", ["pause"]]] }
										,{ "event":"false","targets":["continue"] }
									]
								}

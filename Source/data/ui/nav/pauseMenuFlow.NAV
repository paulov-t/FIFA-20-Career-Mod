			,"states":
			[
				{
					"name":"CheckRegularFlow"
					,"onEnter":
					[
						["sendAction", ["condition", "isRegularFlow"]]
					]
					,"transitions":
					[
						{
							"event":"true"
							,"cond":["==", ".introMatch", "true"]
							,"targets":["gameActive"]
							,"actions":[ ["invoke",["evt_controller_disconnect_flow_ready"]] ]

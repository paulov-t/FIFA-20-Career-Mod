				,{ "event":"evt_online_disconnect_own_goals_eom", "targets":["OwnGoalsEomPopup"] }
				,{ "event":"evt_online_disconnect_own_goals_h2h", "targets":["OwnGoalsH2HPopup"] }
				,{ "event":"evt_online_disconnect_own_goals_h2h_eom", "targets":["OwnGoalsH2HEomPopup"] }
				,{ "event":"evt_online_disconnect_mismatching_changelist", "targets":["MismatchingChangelistPopup"] }
				,{ "event":"evt_online_disconnect_mismatching_changelist_eom", "targets":["MismatchingChangelistEomPopup"] }
				,{ "event":"evt_online_desync", "targets":["OnlineDesyncPopup"] }

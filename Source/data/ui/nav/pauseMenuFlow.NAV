				,{
					"name":"PMASkillGame"
					,"transitions":
					[
						{
							"event":"advanceFromPMA"
							,"targets":["CheckRegularFlow"]
						}
					]
					,"states":
					[
						{
							"name":"WaitForInstructions"
							,"transitions":
							[
								{
									"event":"showInstructions"
									,"targets":["instructionsOverlay"]
								}

								,{ "event":"MatchForfeit","targets":["MatchForfeit"] }
								,{ "event":"ForceSubstitution","targets":["teamSheetEditor"] }
								,{ "event":"VotingMenu","targets":["VotingMenu"] }
								,{ "event":"smHeroRemovedFromMatch","targets":["HeroOffField"] }

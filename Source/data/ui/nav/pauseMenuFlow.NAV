							]
							,"transitions":
							[
								{
									"event":"advanceToMatch"
									,"targets":["PreMatchUpsellWait"]
								}
							]
						}
						,{
							"name":"PreMatchUpsellWait"
							,"onExit":
							[
								["sendAction", ["evt_hub_hide_event"]]
							]
							,"transitions":
							[
								{
									"event":"TriggerEnterGame"
									,"targets":["gameActive"]
								}
							]
						}

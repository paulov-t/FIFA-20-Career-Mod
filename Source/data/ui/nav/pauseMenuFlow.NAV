							"event":"false"
							,"targets":["WaitForScenes"]
						}
					]
				}
				,{
					"transitions":
					[
						{
							"event":"TriggerEnterGame"
							,"targets":["gameActive"]
						}
					]
					,"states":
					[
						{
							"name":"WaitForScenes"
							,"onEnter":
							[
							
								["sendAction", ["evt_hub_hide_event"]]
							]
						}
					]
				}
				,{
					"name":"PauseProfileSignOutWait"
					,"transitions":
					[
						{
							"event":"profileSignOutComplete"
							, "actions":[ ["invoke",["GoToTitleScreen"]]]
						}
					]
				}
				,{
					"name":"PMASkillGameCheck"
					,"onEnter":
					[
						["sendAction", ["condition", "hasPMASkillGame"]]

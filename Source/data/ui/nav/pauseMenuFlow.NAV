					[
						["invoke", ["evt_controller_disconnect_flow_not_ready"]]
						,["sendScreenEvent", ["RestartMatch"]]
						,["sendAction", ["transitionManager", ".NavTransitionManager.TO_GAME"]]
						,["sendAction", ["fbStartFlowEvent", "gmLoadIntroFlowPrematch"]] 
						,["sendAction", ["fbStartFlowEvent", "gmIntroFlowPreMatchVideo"]]
					]
					,"onExit":
					[
						["invoke",["evt_controller_disconnect_flow_ready"]]
					]
					,"transitions":
					[
						{"event":"videoStopped","targets":["gameActive"]}
					]
				}
				,{
					"name":"gameActive"
					,"initial":
					{
						"targets":["InGame"]
					}
					,"onEnter":
					[   
						["sendAction", ["transitionManager", ".NavTransitionManager.TO_GAME"]]
					]
					,"onExit":
					[
						["sendAction", ["transitionManager", ".NavTransitionManager.TO_FE"]]

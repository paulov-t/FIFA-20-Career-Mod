									,"states":
									[
										{
											"name":"PlayCinematic"
											,"type":"external"
											,"file":"cinematicState.nav"
											,"outputs":
											{
												"out_advance":"InGame"
												,"out_back":"InGame"
											}
										}
									]       
								}
							]
						}   
						,{
							"name":"PostGame"
							,"states":
							[
								{
									"name":"Draw"
									,"onEnter":
									[
										["sendScreenEvent", ["UxSetFlowSubState",".eSubState.DRAW"]]
									]
								}
								,{
									"name":"HalfTime"
									,"onEnter":
									[
										["sendScreenEvent", ["UxSetFlowSubState",".eSubState.HALF_TIME"]]
									]
								}
								,{
									"name":"MatchEnded"
									,"onEnter":
									[
										["sendScreenEvent", ["UxSetFlowSubState",".eSubState.POSTGAME"]]
									]
								}

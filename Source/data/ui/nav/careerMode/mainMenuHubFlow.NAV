				]
				,"onExit":
				[
					["unloadViewModel",["KitSelectViewModel"]]
				]
				,"states":
				[
					{
						"name":"PreSeasonTourFlow"
						,"onEnter":
						[
							["executeCMAction",["EnterPreSeasonTourFlow"]]
						]						
						,"transitions":
						[
							{
								"event":"EnterPreSeasonTourInvite"
								,"targets":["PreSeasonTourInvite"]
							}
							,{
								"event":"Advance"
								,"targets":["MainMenuHub"]
							}
						]
					}
					,{
						"name":"MainMenuHub"
						,"initial": 
						{ 
							"targets":["MainMenuHubCheckScenesUnloaded"] 
						}
						,"states":
						[
							{
								"name":"MainMenuHubCheckScenesUnloaded"
								,"onEnter":
								[
									["sendAction",["condition", "areScenesUnloadedExtraCheck"]]
								]
								,"transitions":
								[
									{
										"event":"scenesUnloaded",
										"targets":["MainMenuHubCheckVideo"]
									}
									,{
										"event":"scenesNotUnloaded",
										"targets":["waitForScenesUnload"]
									}		
									,{
										"event":"skipScenesCheck",
										"targets":["MainMenuHubCheckVideo"]
									}
								]
							}
							,{
								"name":"waitForScenesUnload"
								,"transitions":
								[
									{
										"event":"scenesUnloaded",
										"targets":["MainMenuHubCheckVideo"]
									}			
								]
							}
							,{
								"name":"MainMenuHubCheckVideo"
								,"onEnter":
								[
									["executeCMAction",["PlayRegionalVideoCheck"]]
								]
								,"transitions":
								[
									{"event":"SkipRegionalVideo","targets":["MainMenuHubIdle"]}
									,{"event":"PlayUEFAVideo","targets":["MainMenuHubPlayUEFAVideo"]}
									,{"event":"PlayCONMEBOLVideo","targets":["MainMenuHubPlayCONMEBOLVideo"]}
								]
							}
							,{
								"name":"MainMenuHubPlayVideo"
								,"onEnter":
								[
									["sendAction", ["StopMenuMusic"]]
								]
								,"onExit":
								[
									["sendAction", ["StartMenuMusic"]]
								]
								,"states":
								[
									{
										"name":"MainMenuHubPlayUEFAVideo"
										,"onEnter": 
										[
											["sendAction", ["fbStartFlowEvent", "gmIntroTournamentVideo"]]
										]
									}
									,{
										"name":"MainMenuHubPlayCONMEBOLVideo"
										,"onEnter": 
										[
											["sendAction", ["fbStartFlowEvent", "gmIntroConmebolVideo"]]
										]
									}
								]
								,"transitions":
								[
									{"event":"videoStopped","targets":["MainMenuHubIdle"], "let":{ "CareerHubAction":"ResetPanelIndex" }}
								]
							}
							,{
								"name":"MainMenuHubIdle"
								,"initial": { "targets":["WaitForKitsBeforeCareerHub"] }

										[
											{
												"event":"advance"
												,"actions":[ ["invoke", ["EnterPreMatchPressConf"]] ]
											}
											,{
												"event":"back"
												,"targets":["CareerHubIdle"]
											}
										]
									}
									,{
										"name":"CareerHub"
										,"onEnter":

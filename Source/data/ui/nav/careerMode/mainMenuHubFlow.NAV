									,["unloadViewModel",["StartingXIViewModel"]]
									,["unloadViewModel",["FluxViewModel"]]
									,["unloadScreenController",["MainMenuHub"]]
								]
								,"states":
								[
									{
										"name":"ActionPopup"
										,"type":"external"
										,"file":"/careerMode/actionPopupFlow.nav"
										,"outputs": 
										{ 
											"BackFromPopup":"CareerHub"
											,"BackFromScreen":"MainMenuHubWithPopup"
											,"BackFromScreenWithoutPopup":"MainMenuHubWithPopup"

			{
				"success":"loadWomenTournamentModeFlow"
				,"fail":"back"
			}
		}
		,{
			"name":"preLoadSquads"
			,"type":"external"
			,"file":"/performProfileCheckFlow.nav"
			,"outputs":
			{
				"success":"LoadSquads"
				,"fail":"back"
			}
		}
		,{
			"name":"preSaveSquads"
			,"type":"external"
			,"file":"/performProfileCheckFlow.nav"
			,"outputs":
			{
				"success":"SaveSquads"
				,"fail":"back"
			}
		}
		,{
			"name":"preLoadProfile"
			,"type":"external"
			,"file":"/performProfileCheckFlow.nav"
			,"outputs":
			{

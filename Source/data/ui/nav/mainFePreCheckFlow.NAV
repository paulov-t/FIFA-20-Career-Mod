				,"fail":"back"
			}
		}
		,{
			"name":"preSaveProfile"
			,"type":"external"
			,"file":"/performProfileCheckFlow.nav"
			,"outputs":
			{
				"success":"checkSaveLeadProfile"
				,"fail":"back"
			}
		}
		,{
			"name":"checkSaveLeadProfile"
			,"onEnter":
			[
				["sendScreenEvent", ["UXHasLeadProfile"]]
			]
			,"transitions":
			[
				{
					"event":"advance"
					,"targets":["saveProfileSettings"]
				}	
				,{
					"event":"back"
					,"targets":["noLeadProfilePopup"]
				}	
			]
		}
		,{
			"name":"checkLoadLeadProfile"
			,"onEnter":
			[
				["sendScreenEvent", ["UXHasLeadProfile"]]
			]
			,"transitions":
			[
				{
					"event":"advance"
					,"targets":["loadProfileSettings"]
				}	
				,{
					"event":"back"
					,"targets":["noLeadProfilePopup"]
				}	
			]
		}
		,{
			"name":"preCheckContinueCareerModeFlow"
			,"type":"external"
			,"file":"/performProfileCheckFlow.nav"
			,"outputs":
			{
				"success":"LoadLatestCareer"
				,"fail":"back"
			}
		}
		,{
			"name":"LoadLatestCareer"
			,"onEnter":
			[
				["sendScreenEvent", ["UXLoadLatestCareer"]]
			]
			,"transitions":
			[
				{
					"event":"advance"
					,"targets":["continueCareerModeFlow"]
				}	
				,{
					"event":"back"
					,"targets":["back"]
				}	
			]
		}
	]
}
